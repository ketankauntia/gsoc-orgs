model upload_history {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  new_count      Int
  original_count Int
  source_file    String
  timestamp      DateTime @db.Date

  @@map("_upload_history")
}

model organizations {
  id                     String               @id @default(auto()) @map("_id") @db.ObjectId
  active_years           Int[]
  category               String
  contact                OrganizationsContact
  created_at             String
  description            String
  first_year             Int
  id_                    String               @unique(map: "id_1") @map("id")
  image_background_color String
  image_slug             String
  image_url              String
  img_r2_filename        String
  img_r2_url             String
  img_uploaded_at        DateTime             @db.Date
  is_currently_active    Boolean
  last_updated           String
  last_year              Int
  logo_local_filename    String?
  logo_r2_url            String?
  logo_uploaded_at       DateTime?            @db.Date
  /// Could not determine type: the field only had null or empty values in the sample set.
  mentors                Json?
  name                   String
  slug                   String               @unique(map: "slug_1")
  social                 OrganizationsSocial
  stats                  OrganizationsStats
  technologies           String[]
  topics                 String[]
  total_projects         Int
  url                    String
  years                  OrganizationsYears

  @@index([name], map: "name_1")
  @@index([category], map: "category_1")
  @@index([is_currently_active], map: "is_currently_active_1")
  @@index([total_projects(sort: Desc)], map: "total_projects_-1")
}

model organizations_old_backup {
  id                    String                              @id @default(auto()) @map("_id") @db.ObjectId
  appearances           OrganizationsOldBackupAppearances[]
  canonical_id          String                              @unique(map: "canonical_id_1")
  categories            String[]
  contributor_guide_url String
  created_at            DateTime                            @db.Date
  description_html      String
  logoUrl               String
  logo_bg_color         String?
  logo_local_filename   String?
  logo_r2_url           String?
  logo_uploaded_at      DateTime?                           @db.Date
  name                  String
  short_desc            String
  slug                  String
  socials               OrganizationsOldBackupSocials
  tech_stack            String[]
  topics                String[]
  updated_at            DateTime                            @db.Date
  website               String
  years_appeared        Int[]
  years_count           Int

  @@index([slug], map: "slug_1")
  @@index([website], map: "website_1")
  @@index([years_appeared], map: "years_appeared_1")
}

model projects {
  id                     String    @id @default(auto()) @map("_id") @db.ObjectId
  contributor            String
  created_at             DateTime  @db.Date
  date_archived          DateTime? @db.Date
  date_created           DateTime  @db.Date
  date_updated           DateTime? @db.Date
  lastUpdated            DateTime  @db.Date
  mentors                String[]
  org_canonical_id       String
  org_name               String
  org_slug               String
  project_abstract_short String
  project_code_url       String?
  project_id             String    @unique(map: "project_id_1")
  project_info_html      String
  project_title          String
  year                   Int

  @@index([org_canonical_id], map: "org_canonical_id_1")
  @@index([year], map: "year_1")
}
